<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <TabPane prefHeight="601.0" prefWidth="800.0" BorderPane.alignment="CENTER">
        <tabs>
          <Tab text="%reserve_view">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="561.0" prefWidth="800.0">
                     <children>
                        <BorderPane prefHeight="574.0" prefWidth="800.0">
                           <top>
                              <Label alignment="CENTER" text="%driver_view_label" BorderPane.alignment="CENTER">
                                 <padding>
                                    <Insets bottom="20.0" top="20.0" />
                                 </padding>
                              </Label>
                           </top>
                           <left>
                              <TableView fx:id="reserve_tableview" prefHeight="500.0" prefWidth="400.0" BorderPane.alignment="CENTER">
                                <columns>
                                  <TableColumn fx:id="shift_time" minWidth="30.0" prefWidth="200.0" text="%time_info" />
                                  <TableColumn fx:id="customer_name" minWidth="30.0" prefWidth="200.0" text="%client_name" />
                                </columns>
                                 <BorderPane.margin>
                                    <Insets bottom="20.0" left="50.0" />
                                 </BorderPane.margin>
                              </TableView>
                           </left>
                           <center>
                              <VBox prefHeight="506.0" prefWidth="0.0" spacing="20.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Label text="%drivingshift_info">
                                       <font>
                                          <Font size="16.0" />
                                       </font>
                                    </Label>
                                    <Label layoutX="10.0" layoutY="10.0" text="%customer_name" />
                                    <Label layoutX="10.0" layoutY="10.0" text="%shift_id" />
                                    <Label layoutX="10.0" layoutY="28.0" text="%cargo_text" />
                                    <Label layoutX="10.0" layoutY="45.0" text="%time_ttext" />
                                    <Button fx:id="reserve_button" mnemonicParsing="false" text="%reserve_button" wrapText="true">
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin>
                                    </Button>
                                 </children>
                                 <padding>
                                    <Insets left="50.0" />
                                 </padding>
                                 <BorderPane.margin>
                                    <Insets top="20.0" />
                                 </BorderPane.margin>
                              </VBox>
                           </center>
                        </BorderPane>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="%reporting_view">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <BorderPane prefHeight="572.0" prefWidth="800.0">
                           <top>
                              <Label alignment="CENTER" text="%driver_view_label" BorderPane.alignment="CENTER">
                                 <padding>
                                    <Insets top="20.0" />
                                 </padding>
                                 <BorderPane.margin>
                                    <Insets bottom="20.0" />
                                 </BorderPane.margin>
                              </Label>
                           </top>
                           <left>
                              <TableView fx:id="report_tableview" prefHeight="500.0" prefWidth="400.0" BorderPane.alignment="CENTER">
                                <columns>
                                    <TableColumn fx:id="time_info" minWidth="30.0" prefWidth="200.0" text="%time_info" />
                                    <TableColumn fx:id="customer_name" minWidth="30.0" prefWidth="200.0" text="%client_name" />
                                </columns>
                                 <BorderPane.margin>
                                    <Insets bottom="20.0" left="50.0" />
                                 </BorderPane.margin>
                              </TableView>
                           </left>
                           <center>
                              <VBox prefHeight="200.0" prefWidth="100.0" spacing="20.0" BorderPane.alignment="CENTER">
                                 <children>
                                    <Label text="%drivingshift_info">
                                       <font>
                                          <Font size="16.0" />
                                       </font>
                                    </Label>
                                    <Label text="%customer_name" />
                                    <Label text="%shift_id" />
                                    <Label text="%cargo_text" />
                                    <Label text="%time_ttext" />
                                    <HBox prefHeight="100.0" prefWidth="200.0" spacing="20.0">
                                       <children>
                                          <Button fx:id="report_button" mnemonicParsing="false" text="%report_button" wrapText="true" />
                                          <Button fx:id="unassign_button" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="%unassign_button" wrapText="true" />
                                       </children>
                                    </HBox>
                                 </children>
                                 <padding>
                                    <Insets left="50.0" />
                                 </padding>
                                 <BorderPane.margin>
                                    <Insets top="20.0" />
                                 </BorderPane.margin>
                              </VBox>
                           </center>
                        </BorderPane>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </top>
</BorderPane>
